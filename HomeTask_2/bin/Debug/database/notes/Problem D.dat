import java.math.BigInteger;
import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        BigInteger n = new BigInteger("1");
        int a = sc.nextInt();
        while (a > 0)
        {
            n = n.multiply(BigInteger.valueOf(a));
            a--;
        }
        System.out.println(n);
    }
}